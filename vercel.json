{
  "version": 2,
  "builds": [{ "src": "api/vercel.py", "use": "@vercel/python" }],
  "routes": [
    { "src": "/api/(.*)", "dest": "api/vercel.py" },
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "/static/$1" },
    { "src": "/", "dest": "/static/index.html" }
  ],
  "env": {
    "PYTHONPATH": "."
  }
}
